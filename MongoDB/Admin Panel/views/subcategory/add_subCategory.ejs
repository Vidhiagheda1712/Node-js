<%- include('../header') %>
  <div class="main-panel">
    <div class="content-wrapper">
      <div class="page-header">
        <h3 class="page-title text-center"></h3>
      </div>
 <div class="row d-flex justify-content-center">
  <div class="col-md-9">
    <% if(locals.message?.success){ %>
      <div class="alert alert-success w-50">
        <%= locals.message?.success %>
      </div>
      <% } %>
   </div>
 </div>
          <div class="row d-flex justify-content-center">
            <div class="col-md-9  stretch-card">
              <div class="card" style="height:300px;">
                <div class="card-body">
                  <h4 class="card-title " align="center">Sub Category</h4>
                  <form class="forms-sample" method="post" action="/subcategory/insertsubcategory">
                    <div class="form-group">
                      <label for="fname" class=" text-right control-label col-form-label">
                        Category</label>
                    <div>
                      <select name="category" class="form-control">
                        <option>---select category---</option>
                        <% category.map((val)=>{ %>
                            <option value="<%= val?.id %>">
                                <%= val?.category %>
                            </option>
                            <% }) %>
                    </select>
                   </div>
                    </div>
                    <div class="form-group">
                      <label for="exampleInputUsername1">Sub Category Name</label>
                      <input type="text" class="form-control" id="exampleInputUsername1" name="subcategory"
                        placeholder="Sub Category Name">
                    </div>
                    <button type="submit" class="btn btn-primary mr-3">Submit</button>
                  </form>
                </div>
              </div>
            </div>
          </div>
    </div>
    <!-- content-wrapper ends -->
    <!-- partial:../../partials/_footer.html -->
