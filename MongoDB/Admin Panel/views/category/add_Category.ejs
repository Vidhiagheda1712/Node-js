<%- include('../header') %>
  <div class="main-panel">
    <div class="content-wrapper">
      <div class="page-header">
        <h3 class="page-title text-center"></h3>
      </div>
      <div class="row  d-flex justify-content-center align-items-center">
        <div class="col-md-9">
          <% if(locals.message?.success){ %>
            <div class="alert alert-success w-50">
              <%= locals.message?.success %>
            </div>
            <% } %>
        </div>
      </div>
      <div class="row d-flex justify-content-center">
        <div class="col-md-9  stretch-card ">
          <div class="card" style="height: auto;">
            <div class="card-body">
              <h4 class="card-title " align="center">Category</h4>

              <form class="forms-sample" method="post" action="/category/insertcategory">
                <div class="form-group">
                  <label for="exampleInputUsername1">Category Name</label>
                  <input type="text" class="form-control" id="exampleInputUsername1" name="category"
                    placeholder="Category" style="margin-top: 15px;">
                </div>
                <button type="submit" class="btn btn-primary mr-2 px-3 py-2" style="margin-top: 25px;">Submit</button>
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- content-wrapper ends -->
    <!-- partial:../../partials/_footer.html -->
    <footer class="footer">
      <div class="d-sm-flex justify-content-center justify-content-sm-between">
        <span class="text-muted d-block text-center text-sm-left d-sm-inline-block">Copyright Â© bootstrapdash.com
          2020</span>
        <span class="float-none float-sm-right d-block mt-1 mt-sm-0 text-center"> Free <a
            href="https://www.bootstrapdash.com/bootstrap-admin-template/" target="_blank">Bootstrap admin templates</a>
          from Bootstrapdash.com</span>
      </div>
    </footer>
    <!-- partial -->
  </div>
  <%- include('../footer') %>